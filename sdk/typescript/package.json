{
  "name": "@apex/typescript-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Project Apex API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint src tests --ext .ts"
  },
  "keywords": [
    "apex",
    "api",
    "sdk",
    "typescript",
    "agents",
    "tasks",
    "dag"
  ],
  "author": "Aezi <aezi.zhu@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "axios-mock-adapter": "^1.22.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
